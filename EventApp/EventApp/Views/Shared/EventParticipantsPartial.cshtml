@model List<Participant>
<!--Här ligger listan med anmälda users-->
<div class="row">
    <hr />
    <div class="col-md-12">
        <h3>Anmälda deltagare</h3>
        <div class="media-body">
            <ul class="list-group">
                @foreach (Participant participant in Model)
                {
                   <li class="list-group-item">@participant.UserId <span class="badge">Creator</span></li>
                }
            </ul>
        </div>
    </div>
    @if (Model.Count == 0)
    {
        <p>Inga anmälda</p>
    }

</div>